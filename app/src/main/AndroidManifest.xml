<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tyr.example.kotlinexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotlinExample">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity">

        </activity>

        <activity android:name=".CalCulateActivity"></activity>
        <activity android:name=".ArchiveActivity"></activity>
        <activity android:name=".WeatherActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>